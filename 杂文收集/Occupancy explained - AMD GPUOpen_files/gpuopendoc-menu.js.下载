function setCookie(cname,cvalue,exdays=null){if(exdays!=null){const d=new Date();d.setTime(d.getTime()+(exdays*24*60*60*1000));let expires="expires="+d.toUTCString();document.cookie=cname+"="+cvalue+";"+expires+";path=/"}else{document.cookie=cname+"="+cvalue+";path=/"}}
function getCookie(cname){let name=cname+"=";let decodedCookie=decodeURIComponent(document.cookie);let ca=decodedCookie.split(';');for(let i=0;i<ca.length;i++){let c=ca[i];while(c.charAt(0)==' '){c=c.substring(1)}
if(c.indexOf(name)==0){return c.substring(name.length,c.length)}}
return""}
jQuery(document).ready(function($){var numh2s=jQuery("#docs_content_column h2").length;if(numh2s<=1){jQuery("#docs-right-toc").css("display","none")}
jQuery("div.elementor-widget-table-of-contents div.elementor-toc__toggle-button").click(function(){if(jQuery("div.elementor-widget-table-of-contents").hasClass("elementor-toc--collapsed")){setCookie("toc-collapsed","false")}else{setCookie("toc-collapsed","true")}
jQuery("div.elementor-widget-table-of-contents").addClass("autoclicked")});setTimeout(function(){var tocstate=getCookie("toc-collapsed");if(tocstate=="true"){jQuery("div.elementor-widget-table-of-contents div.elementor-toc__toggle-button--collapse").trigger('click')}},500);$(".header i.fas").click(function(){$header=$(this).closest(".header");$content=$header.next(".content");$content.slideToggle(1,function(){$newcontent=$header
$widget=$newcontent.find("i.fas");$widget.toggleClass("fa-minus-circle");$widget.toggleClass("fa-plus-circle")})});$(".doc-toc").on("click",".header svg.fas",function(event){$header=$(this).closest(".header");$content=$header.next(".content");$content.slideToggle(1,function(){$newcontent=$header;$widget=$newcontent.find("svg.fas");$imagedata=$newcontent.find("path.faspath");if($imagedata.hasClass("i-m")){$imagedata.attr("d","M256 512c141.4 0 256-114.6 256-256S397.4 0 256 0S0 114.6 0 256S114.6 512 256 512zM232 344V280H168c-13.3 0-24-10.7-24-24s10.7-24 24-24h64V168c0-13.3 10.7-24 24-24s24 10.7 24 24v64h64c13.3 0 24 10.7 24 24s-10.7 24-24 24H280v64c0 13.3-10.7 24-24 24s-24-10.7-24-24z");$imagedata.toggleClass("i-m i-p")}else{$imagedata.attr("d","M256 512c141.4 0 256-114.6 256-256S397.4 0 256 0S0 114.6 0 256S114.6 512 256 512zM184 232H328c13.3 0 24 10.7 24 24s-10.7 24-24 24H184c-13.3 0-24-10.7-24-24s10.7-24 24-24z");$imagedata.toggleClass("i-p i-m")}})});$(".code-header a.copy-button-link").click(function(){$nearest_codeheader=$(this).closest(".code-header");$nearest_code=$nearest_codeheader.siblings("pre").find("code");var range=document.createRange();range.selectNode($nearest_code[0]);window.getSelection().removeAllRanges();window.getSelection().addRange(range);document.execCommand("copy");window.getSelection().removeAllRanges();$copy_notification=$nearest_codeheader.find(".code-copied");$cloned_notification=$copy_notification.clone(!0);$cloned_notification.addClass("animated")
$copy_notification.before($cloned_notification);$copy_notification.remove()})})